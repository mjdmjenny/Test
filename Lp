package cjDay6;
class Switch2{
	
}
public class Test1 {
	static int num;
	public static void main(String[] args) {
		int month = 2, year =2015;
		
		switch(month) {
		case 2 : num = (year%4 == 0)?29:28;break;
		case 4: case 6: case 8: num =30; break;
		default: num = 31;
		}
		System.out.println("Print Month " +month + " Print Year "+ year + 
				" Number of Days :: "+ num);
	}

}
